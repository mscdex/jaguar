#!/usr/bin/env bash

echo
#MW=$1 node $2 &
#MW=$1 ~/tmp/node/node-v0.11.14-linux-x64/bin/node $2 &
MW=$1 ~/tmp/node/node-v0.10.31-linux-x64/bin/node $2 &
#MW=$1 ~/tmp/node/node-v0.10.32-linux-x64/bin/node $2 &
#MW=$1 ~/git/node/out/Release/node $2 &
pid=$!

sleep 2

wrk 'http://localhost:3333/?foo[bar]=baz' \
  -d 3 \
  -c 50 \
  -t 4 \
  | grep 'Requests/sec' \
  | awk '{ print "  " $2 }'

kill $pid

